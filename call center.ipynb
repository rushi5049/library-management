class Queue:
    def __init__(self,size=10):
        self.Qsize=size
        self.front= -1
        self.rear= -1
        self.array= [0]*size

    def Qempty(self):
         if(self.front==self.rear):
             print("Qempty")
             return(True)
             return(False)
    def Qfull(self):
          if(self.rear==self.Qsize-1):
            return(True)
            return(False)
    def EnQue(self,data):
        if(not self.Qfull()):
            self.rear+= 1
            self.array[self.rear]= data
        else:
            print("\n Q full")
    def DeQue(self):
            if(not self.Qempty()):
                self.front+=1
                print("deleting Queue")
                self.array[self.front]=0
            else:
                print("\n Qempty ")
    def displayQ(self):
        for i in self.array:
            print(i)
                
                
                

def getCurrTime(format=1):
    from datetime import datetime as dt
    if(format==1):
        return(dt.now().strftime("%d-%m-%Y"),dt.now().strftime("%H:%M:%S"))
    elif(format>=2):
         return(dt.now().strftime("%d-%m-%Y"),dt.now().strftime("%M:%S"))
date_time= getCurrTime(2)
print(date_time[0])

        

callQ=Queue(10)
custId=0

while(1):
    menu="1.add call\n2.answer\n3.pending\n4.qstack\n5.exit"
    print(menu)
    op = int(input("enter option"))
    
    if(op==1):
        custId+=1
        call_time=getCurrTime(2)
        record=(custId,call_time[1])
        callQ.EnQue(record)
        callQ.displayQ()
    elif(op==2):
